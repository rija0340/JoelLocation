	<body>
		{% if isPreview %}
		<div class="a4-paper">
		{% endif %}
		<section>
			<div class="entete"><img src="{{ entete }}" alt="entete" width="800px" height="40px" type="images/png"></div>
		</section>
		<section id="body">
			<div>
				<div class="logo"><img src="{{ logo }}" alt="logo" width="auto" height="80px" type="images/png"></div>
				<div class="adresse">
					<strong class="joellocation">JOELLOCATION</strong>
					<br>
					94 Chemin de la hache<br>
					97160 Le Moule (Guadeloupe)
					<br>
					Tél: 0690737674 / 0767321447<br>
					joel@joellocation.com<br>
					joellocation.com
				</div>
				<div class="contrat_location">
					<h2 class="p-0">Contrat de location
					</h2>
					<div class="d-inline-block">
						<table id="contrat_location_table1" class="table-border">
							<tbody>
								<tr>
									<td class="bg-danger  text-white text-center fs-12">Numéro de contrat</td>
								</tr>
								{% if devis is defined and devis is not null %}
									<tr>
										<td>
											{{ devis.numContrat }}
										</td>
									</tr>
								</tr>
							{% else %}
								<tr>
									<td>
										<p>{{ numContrat }}</p>
									</td>
								</tr>
							{% endif %}
						</tbody>
					</table>
				</div>
				<div class="d-inline-block">
					<table id="contrat_location_table2" class="table-border">
						<tbody>
							<tr>
								<td class="bg-danger text-white text-center fs-12">Date de contrat</td>
							</tr>
							<tr>
								<td>{{ reservation.dateDebut|date('d/m/Y') }}</td>
							</tr>

						</tbody>
					</table>
				</div>
			</div>

			<div style="margin-top: 5px; margin-bottom: 5px" class="text-center">Location de voiture du
				{{ reservation.dateDebut|date('d/m/Y') }}
				à
				{{ reservation.dateDebut|date('H:i') }}
				au
				{{ reservation.dateFin|date('d/m/Y') }}
				à
				{{ reservation.dateFin|date('H:i') }}</div>

			{# table gauche droite, info client et conducteurs #}
			<div class="locataire p-0 ">
				<table id="locataire_table" class="table-border p-0 " style="width : 100%;">
					<tbody class="fs-12">
						<tr style="background-color : red" class="text-center">
							<td colspan="2" class="text-white">LOCATAIRE</td>
						</tr>
						<tr style="padding:2px">
							<td style="padding:2px">
								<span>
									{% if devis is defined and devis is not null %}
										{% if  devis.client.sexe == ""  %}

											{% elseif devis.client.sexe == 'masculin' %}
												Mr
											{% elseif devis.client.sexe == 'feminin' %}
												Mlle/Mme
										{% endif %}
									{% endif %}
								</span>
								Nom</td>
							<td style="padding:2px">{{ reservation.client.nom }}</td>
						</tr>
						<tr>
							<td>Prénom</td>
							<td>{{ reservation.client.prenom }}</td>
						</tr>
						<tr>
							<td>Adresse permanante</td>
							<td>{{ reservation.client.adresse }}</td>
						</tr>
						<tr>
							<td>Ville et code postal</td>
							<td>{{ reservation.client.ville }}
								{{ reservation.client.codePostal }}</td>
						</tr>
						<tr>
							<td></td>
							<td>{{ reservation.client.complementAdresse }}</td>
						</tr>
						<tr>
							<td>Téléhone(s)</td>
							<td>{{ reservation.client.telephone }}</td>
						</tr>
						<tr>
							<td>Email</td>
							<td>{{ reservation.client.mail }}</td>
						</tr>
						<tr>
							<td>Date/lieu de naissance</td>
							<td>{{  reservation.client.dateNaissance ? reservation.client.dateNaissance|date('d/m/Y') : '' }}

								{{ reservation.client.lieuNaissance ?  "à " ~ reservation.client.lieuNaissance  : ""  }}</td>
						</tr>
						<tr>
							<td>N°/Date de permis</td>
							<td>{{ reservation.client.numeroPermis }}

								{{ reservation.client.datePermis ? "le " ~ reservation.client.datePermis|date('d/m/Y') : '' }}</td>
						</tr>
						<tr>
							<td colspan="2" class="text-center text-white bg-danger">INFOS TRANSFERT
							</td>
						</tr>
						<tr>
							<td>Infos arrivée</td>
							<td>
								{{ reservation.client.infosVolResa.heureVolAller ? reservation.client.infosVolResa.heureVolAller|date('d/m/Y') : "  " }}

								{{ reservation.client.infosVolResa.numVolAller ? ", Vol" ~ reservation.client.infosVolResa.numVolAller : "   " }}

								{{ reservation.client.infosVolResa.heureVolAller ? "à" ~ reservation.client.infosVolResa.heureVolAller|date('H:i') : "   " }}
							</td>
						</tr>
						<tr>
							<td>Infos retour
							</td>
							<td>

								{{ reservation.client.infosVolResa.heureVolRetour ? reservation.client.infosVolResa.heureVolRetour|date('d/m/Y') : "  " }}

								{{ reservation.client.infosVolResa.numVolRetour ? ", Vol" ~ reservation.client.infosVolResa.numVolRetour : "   " }}

								{{ reservation.client.infosVolResa.heureVolRetour ? "à"  ~ reservation.client.infosVolResa.heureVolRetour|date('H:i') :  "   " }}

							</td>
						</tr>
					</tbody>
				</table>
			</div>
			{# table droite #}
