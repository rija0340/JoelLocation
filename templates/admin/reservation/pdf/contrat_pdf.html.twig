							</tr>
							<tr>
								<td>
									N°/Date de permis
								</td>
								<td></td>
							</tr>
							<tr>
								<td>
									Téléphone(s)
								</td>
								<td></td>
							</tr>
							<tr>
								<td>Nom</td>
								<td></td>
							</tr>
							<tr>
								<td>Prénom</td>
								<td></td>
							</tr>
							<tr>
								<td>Date/lieu de naissance</td>
								<td></td>
							</tr>
							<tr>
								<td>
									N°/Date de permis
								</td>
								<td></td>
							</tr>
							<tr>
								<td>
									Téléphone
								</td>
								<td></td>
							</tr>
							<tr>
								<td colspan="2" class="bg-danger text-center text-white text-uppercase">Adresse en guadeloupe</td>
							</tr>
							<tr>
								<td colspan="2" style="height: 28px">
									<div class='text-white'>
										lorem lorem lorem lorem
									</div>
								</td>
							</tr>
						{% else %}
							{% set loopCount = 0 %}
							{% for conducteur in reservation.conducteursClient %}
								{% set loopCount = loopCount + 1 %}
								<tr>
									<td style="min-height: 5px">Nom</td>
									<td style="min-height: 5px">{{ conducteur.nom }}</td>
								</tr>
								<tr>
									<td>Prénom</td>
									<td>{{ conducteur.prenom }}</td>
								</tr>
								<tr>
									<td>Date/lieu de naissance</td>
									<td>{{ conducteur.dateNaissance|date('d/m/Y') }}
