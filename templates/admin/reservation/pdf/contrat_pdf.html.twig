									<td>Téléphone</td>
									<td>{{ conducteur.telephone }}</td>
								</tr>
							{% endfor %}

							{% if loopCount < 2 %}
								<tr>
									<td style="min-height: 5px">Nom</td>
									<td style="min-height: 5px"></td>
								</tr>
								<tr>
									<td>Prénom</td>
									<td></td>
								</tr>
								<tr>
									<td>Date/lieu de naissance</td>
									<td></td>
								</tr>
								<tr>
									<td>N°/Date de permis</td>
									<td></td>
								</tr>
							{% endif %}
							<tr>
								<td colspan="2" class="bg-danger text-center text-white text-uppercase">adresse en guadeloupe</td>
							</tr>
							<tr>
								<td colspan="2">
									<div class='text-white' style="min-height: 28px;">
										lorem lorem lorem lorem
									</div>
								</td>
							</tr>
						{% endif %}
					</tbody>
					{# </tbody> #}
				</table>
			</div>
